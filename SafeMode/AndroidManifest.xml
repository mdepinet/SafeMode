<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.teamPrime.sm"
      android:versionCode="3"
      android:versionName="1.0.2">
    <uses-sdk android:minSdkVersion="6" />

	<uses-permission android:name="android.permission.READ_CONTACTS" />
	<uses-permission android:name="android.permission.WRITE_CONTACTS" />
	<uses-permission android:name="android.permission.SEND_SMS"/>
	<uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />
	<uses-permission android:name="android.permission.READ_SMS" />
	<uses-permission android:name="android.permission.WRITE_SMS" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<!-- <uses-permission android:name="com.android.vending.CHECK_LICENSE" /> -->
	

	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity	android:name=".SafeLaunchActivity"
					android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" /> 
			</intent-filter> 
        </activity>
		<activity	android:name=".MathStopActivity"
					android:label="@string/app_name" />
		<activity	android:name = ".BlackListActivity"
					android:label="@string/app_name" />
		<activity	android:name=".HistoryActivity"
         			android:label="@string/app_name" />
        <activity	android:name=".FindMeActivity"
         			android:label="@string/app_name" />
         			
        <receiver android:exported="true" android:name=".UserPresentReceiver">
			<intent-filter>
				<action android:name="android.intent.action.USER_PRESENT" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</receiver>
	</application>
</manifest>